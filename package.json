    "redux": "^2.0.0 || ^3.0.0"
  },
  "devDependencies": {
    "babel-cli": "6.18.0",
    "babel-plugin-addnpm-module-exports": "0.2.1",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-stage-0": "^6.5.0",
    "eslint-config-airbnb": "14.0.0",
    "eslint-plugin-react": "6.9.0",
    "expect": "^1.9.0",
    "immutable": "^3.7.5",
    "mocha": "^2.3.3",
